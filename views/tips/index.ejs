

<ul>
    <% for (var i in tips) { %>
          <% var tip = tips[i]; %>

          <li>
                 <%= tip.text %>

                 <% if (!tip.accepted) { %>
                        <a href="/quizzes/<%= quizId %>/tips/<%= tip.id %>/accept?_method=PUT"
                           class="button">Accept</a>
                 <% } %>

                 <a href="/quizzes/<%= quizId %>/tips/<%= tip.id %>?_method=DELETE"
                    class="button">Delete</a>

          </li>
    <% } %>
</ul>